rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /rdtechinnovations/assets/{allPaths=**} {
      allow read: if true;  // Public read access for website assets
      allow write: if request.auth != null;
    }
    match /rdtechinnovations/pdfs/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
